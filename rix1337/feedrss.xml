<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Beta>True</Beta>
  <Category>Downloaders</Category>
  <Name>FeedRss</Name>
  <Description>
	[b]This container is highly experimental[br]
	It uses RSS feeds to create JDownloader crawljobs.
  </Description>
  <Overview>
  Experimental: FeedRss uses RSS feeds to create JDownloader crawljobs.
  </Overview>
  <Support>https://lime-technology.com/forum/index.php?topic=43669.0</Support>
  <Registry>https://registry.hub.docker.com/u/rix1337/docker-feedrss</Registry>
  <GitHub>https://github.com/rix1337/docker-feedrss</GitHub>
  <Repository>rix1337/docker-feedrss</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/feedrss</HostDir>
      <ContainerDir>/config/src</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/appdata/jdownloader/jd2/folderwatch</HostDir>
      <ContainerDir>/jd2</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Banner></Banner>
  <Icon>https://raw.githubusercontent.com/rix1337/docker-templates/master/rix1337/feedrss.png</Icon>
  <Date>2016-03-11</Date>
  <Changes>
	-Initial Release
  </Changes>
</Containers>

<?xml version="1.0"?>
<Container version="2">
  <Name>BudgeTeer</Name>
  <Repository>rix1337/docker-budgeteer:latest</Repository>
  <Registry>https://registry.hub.docker.com/u/rix1337/docker-budgeteer</Registry>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview>Einfacher &#xDC;berblick &#xFC;ber das eigene Budget </Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:2808]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/rix1337/BudgeTeer/main/budgeteer/web_interface/vuejs_frontend/public/favicon.ico</Icon>
  <ExtraParams>--log-opt max-size=50m</ExtraParams>
  <PostArgs/>
  <CPUset></CPUset>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="LOGLEVEL" Target="LOGLEVEL" Default="INFO" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">INFO</Config>
  <Config Name="Config" Target="/config" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/cache/appdata/budgeteer/</Config>
  <Config Name="Web-UI" Target="2808" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">2808</Config>
</Container>

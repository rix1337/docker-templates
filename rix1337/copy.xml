<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
  <Category>Cloud: Downloaders:</Category>
  <Date>2015-10-25</Date>
  <Changes>
    -Initial Release
</Changes>
  <Name>Copy</Name>
  <Support>https://lime-technology.com/forum/index.php?topic=43669.0</Support>
  <Description>
   Copy.com (DISCONTINUED AFTER MAY 1ST 2016! - BACKUP NOW!)
   
   /syncdir is your Copy folder
   
   Switch to Advanced View and set your login info (COPYNAME/COPYPW)!
   
   This container is based on smdion/docker-nginx/
  </Description>
  <Overview>Copy.com (DISCONTINUED AFTER MAY 1ST 2016! - BACKUP NOW!)</Overview>
<Project>http://sabnzbd.org/</Project>
  <Registry>https://hub.docker.com/r/rix1337/docker-copy/</Registry>
  <GitHub>https://github.com/rix1337/docker-copy</GitHub>
  <Repository>rix1337/docker-copy</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>COPYNAME</Name>
      <Value>Your Email Address</Value>
    </Variable>
    <Variable>
      <Name>COPYPW</Name>
      <Value>Your Password</Value>
    </Variable>
  </Environment>
  <Networking>
</Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/Copy</HostDir>
      <ContainerDir>/syncdir</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Banner>http://www.itcentralpoint.com/wp-content/uploads/2013/05/Copy_Horizontal_Banner.png</Banner>
  <Icon>https://lh6.googleusercontent.com/-KbChyLdIAzM/U2D8vNZBEwI/AAAAAAAACDs/t1lPrumNFj0/logo.png</Icon>
